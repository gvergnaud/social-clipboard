{
  "name": "clipboard",
  "version": "1.0.0",
  "description": "shared clipboard app",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf app/dist && rimraf dist",
    "dev:main": "NODE_ENV=development nodemon --exec electron ./src/index.js --ignore ./src/app/",
    "dev:app": "NODE_ENV=development babel-node server.js",
    "start": "NODE_ENV=production electron ./src/index.js",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && npm run package",
    "abi": "npm rebuild --runtime=electron --target=1.4.4 --disturl=https://atom.io/download/atom-shell --abi=50",
    "package": "build --dir"
  },
  "build": {
    "appId": "app.electron.clipboard",
    "mac": {
      "category": "your.app.category.type"
    },
    "asar": false
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "electron-debug": "^1.0.1",
    "express": "^4.14.0",
    "fs-promise": "^0.5.0",
    "keypress": "^0.2.1",
    "lodash": "^4.16.4",
    "menubar": "^5.1.0",
    "node-notifier": "^4.6.1",
    "nodobjc": "^2.1.0",
    "notifier": "^0.1.7",
    "progress-stream": "^1.2.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-observable": "^0.12.1",
    "rxjs": "^5.0.0-rc.1",
    "socket.io": "^1.5.0",
    "socket.io-client": "^1.5.0",
    "socket.io-stream": "^0.9.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "css-loader": "^0.25.0",
    "electron": "^1.4.4",
    "electron-builder": "^7.14.2",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "keywords": [],
  "author": "gvergnaud <gabriel.vergnaud@hetic.net>",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/gvergnaud/social-clipboard.git",
    "type": "git"
  }
}
